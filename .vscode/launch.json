{
	"configurations": [
		{
			"name": "Launch Script on Server",
			"request": "launch",
			"type": "janus",
			"script": "${file}",
			"username": "kayau4271@gmail.com",
			"password": "Yediyaman02.",
			"principal": "TRY",
			"host": "chmod 777 main",
			"applicationPort": 11000,
			"debuggerPort": 8089,
			"tls": true,
			"currentConfiguration": true,
			"stopOnEntry": true,
			"log": {
				"fileName": "${workspaceRoot}/vscode-janus-debug-launch.log",
